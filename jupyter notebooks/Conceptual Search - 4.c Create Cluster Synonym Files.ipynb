    "#top 5k dice keywords\n",
    "NUM_CLUSTERS         = 3000 # for 25k keywords and phrases\n",
    "# number of cluster synonyms to map to\n",
    "NUM_CLUSTER_SYNONYMS = 5\n",
    "KEY_WORDS_FILE       = \"G:\Documents/Dice Data/LuceneTalk/top_5k_keywords.txt\"\n",
    "SYNONYMS_QRY_FILE    = \"G:\Documents/Dice Data/LuceneTalk/cluster_keyword_synonym_qry.txt\"\n",
    "SYNONYMS_INDEX_FILE  = \"G:\Documents/Dice Data/LuceneTalk/cluster_keyword_synonym_ix.txt\"\n",
    "PHRASES_FILE         = \"G:\Documents/Dice Data/LuceneTalk/Phrases.txt\"\n",
    "MODEL_FILE           = \"G:\Documents/Dice Data/LuceneTalk/keyword_model.w2v\"\n",
    "CLUSTERS_FILE        = \"G:\Documents/Dice Data/LuceneTalk/%i_clusters.txt\" % NUM_CLUSTERS"
   ]
